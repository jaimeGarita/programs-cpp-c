
EXPLICACIÓN DEL CÓDIGO

#include <stdio.h> # LIBRERIAS #
#include <stdlib.h> # LIBRERIAS #

int main(){ # EMPEZAMOS CON LA FUNCIÓN #

	int op1, op2, op3, result; # DEFINIMOS VARIABLES LOCALES #
	op1 = 4; # ASIGNAMOS VALORES A LA VARIABLE OP1 #
	op2 = 5; # ASIGNAMOS VALORES A LA VARIABLE OP2 #
	op3 = 7; # ASIGNAMOS VALORES A LA VARIABLE OP3 #

	result = op1 + op2 + op3 ; # ASIGNAMOS VARIABLE AL RESULTADO, DECIMOS QUE RESULTADO ES IGUAL A LA SUMA DE LOS OP #

	printf("%i + %i + %i = %i\n", op1, op2, op3, result); # PRINTEAMOS EL %i 
	VIENE DE NUMERO ENTERO Y LUEGO LE DECIMOS LO QUE VAMOS A AÑADIR DEL %i, EN ESTE CASO SUMAREMOS EL OP1 OP2 Y OP3 #

	return 0; # SI ESTA BIEN NOS DEVUELVA 0 #
}
